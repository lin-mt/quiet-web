{
  "name": "quiet-web",
  "version": "0.0.1",
  "private": true,
  "description": "Quiet's front-end project",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.15.3",
    "@ant-design/pro-descriptions": "^1.9.22",
    "@ant-design/pro-form": "^1.43.2",
    "@ant-design/pro-layout": "^6.26.3",
    "@ant-design/pro-table": "^2.56.0",
    "@monaco-editor/react": "^4.2.2",
    "@quiet-front-end/quiet-json-schema-editor-visual": "^0.0.1",
    "@umijs/route-utils": "^1.0.37",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "genson-js": "^0.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2",
    "styled-components": "^5.3.1",
    "umi": "^3.5.20",
    "umi-serve": "^1.9.10"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.30",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.4",
    "@types/styled-components": "^5.1.15",
    "@umijs/fabric": "^2.8.1",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/plugin-openapi": "^1.3.0",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.30",
    "@umijs/preset-react": "^1.8.24",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.0.1",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.2.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.4.1",
    "puppeteer-core": "^10.4.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
